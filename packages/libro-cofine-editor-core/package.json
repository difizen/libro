{
  "name": "@difizen/libro-cofine-editor-core",
  "version": "0.2.18",
  "description": "",
  "keywords": [
    "libro",
    "notebook",
    "monaco"
  ],
  "repository": "git@github.com:difizen/libro.git",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "typings": "./es/index.d.ts",
      "default": "./es/index.js"
    },
    "./mock": {
      "typings": "./es/mock/index.d.ts",
      "default": "./es/mock/index.js"
    },
    "./es/mock": {
      "typings": "./es/mock/index.d.ts",
      "default": "./es/mock/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "es/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "files": [
    "dist",
    "es",
    "src"
  ],
  "scripts": {
    "setup": "father build",
    "build": "pnpm run build:worker && father build",
    "test": ": Note: lint task is delegated to test:* scripts",
    "test:vitest": "vitest run",
    "test:jest": "jest",
    "coverage": ": Note: lint task is delegated to coverage:* scripts",
    "coverage:vitest": "vitest run --coverage",
    "coverage:jest": "jest --coverage",
    "lint": ": Note: lint task is delegated to lint:* scripts",
    "lint:eslint": "eslint src",
    "build:worker": "webpack --config ./scripts/webpack.worker.cjs",
    "typecheck:tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@difizen/libro-cofine-editor-contribution": "^0.2.18",
    "@difizen/mana-app": "latest",
    "@difizen/monaco-editor-core": "0.39.4",
    "debug": "^4.3.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/debug": "^4.1.6",
    "file-loader": "^6.2.0",
    "source-map-loader": "^4.0.1",
    "ts-loader": "^9.5.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
